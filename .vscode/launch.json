  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Edge",
      "request": "launch",
      "type": "msedge",
      "url": "http://localhost:5174/",
      "webRoot": "${workspaceFolder}"
    },
    {
      "name": "DEV",
      "request": "launch",
      "runtimeArgs": [
        "dev",
      ],
      "runtimeExecutable": "yarn",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    }
  ]
}